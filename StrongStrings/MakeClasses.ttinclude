<#@ include file="MakeAbstract.ttinclude" once="true" #>
<#@ include file="MakeConcrete.ttinclude" once="true" #>
<#+ public void MakeClasses(string usings, string baseClassName, string derivedClassName)
{
	var rootNamespace = Host.ResolveAssemblyReference("$(RootNamespace)");
	var abstractClassName = $"{derivedClassName}Abstract";
	MakeAbstract(usings, rootNamespace, baseClassName, abstractClassName);
	MakeConcrete(usings, rootNamespace, abstractClassName, derivedClassName);
} #>
